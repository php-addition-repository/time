{
  "name": "par/time",
  "type": "library",
  "description": "Time library for the par project",
  "require": {
    "php": "^8.3",
    "ext-intl": "*",
    "psr/clock": "^1.0"
  },
  "provide": {
    "psr/clock-implementation": "1.0"
  },
  "autoload": {
    "psr-4": {
      "Par\\Time\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Par\\Time\\Tests\\": "tests/"
    }
  }
}
